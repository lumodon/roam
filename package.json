{
  "name": "roam",
  "version": "0.1.0",
  "description": "A full-stack travel application.",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
